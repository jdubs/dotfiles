[user]
	name = John-William Trenholm
	email = jw@xomar.com
[alias]
	co = checkout
[apply]
	whitespace = nowarn
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[push]
	default = matching
[core]
	excludesfile = /Users/johntrenholm/.gitignore
